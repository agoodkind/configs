# Internal Link to OPNsense (virtio on mwanbr)
# Generated by Ansible
# Static IP configuration for mwan <-> OPNsense link

[Match]
Name={{ mwan_internal_iface }}

[Network]
# Static IPv4 address
Address={{ mwan_internal_ipv4 }}
# Static IPv6 address
Address={{ mwan_internal_ipv6 }}
# Enable forwarding for policy routing
IPv4Forwarding=yes
IPv6Forwarding=yes
