# OpenSSL configuration for wpa_supplicant
# Enables legacy provider for RSA signature verification
# Required for AT&T EAP-TLS compatibility with OpenSSL 3.x

openssl_conf = openssl_init

[openssl_init]
providers = provider_sect

[provider_sect]
default = default_sect
legacy = legacy_sect

[default_sect]
activate = 1

[legacy_sect]
activate = 1




