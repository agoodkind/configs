# Grommunio Web Configuration
# Generated by Ansible

# Listen address
listen = ::1:8082

# Database connection
mysql_host = localhost
mysql_port = 3306
mysql_user = {{ grommunio_db_user }}
mysql_password = {{ vault_grommunio_db_password }}
mysql_database = {{ grommunio_db_name }}

# Logging
log_file = {{ grommunio_log_dir }}/web.log
log_level = 4
